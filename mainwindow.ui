   <string notr="true">/* 主視窗樣式 - 使用漸層背景增加深度感 */
QMainWindow {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 #f8f9fa, stop:1 #e9ecef);
}

/* 一般標籤樣式 */
QLabel {
    color: #495057;
    font-size: 12pt;
    font-family: &quot;Microsoft JhengHei&quot;, &quot;PingFang TC&quot;, sans-serif;
}

/* 標題標籤 - 增強視覺層次 */
QLabel#titleLabel {
    color: #1a1a2e;
    font-size: 22pt;
    font-weight: bold;
    padding: 10px;
    background-color: qlineargradient(x1:0, y1:0, x2:1, y2:0,
                                      stop:0 #667eea, stop:1 #764ba2);
    border-radius: 10px;
    color: white;
    qproperty-alignment: AlignCenter;
}

/* 時間選擇器 - 改進邊框和陰影效果 */
QTimeEdit {
    background-color: white;
    border: 2px solid #667eea;
    border-radius: 8px;
    padding: 8px 12px;
    font-size: 14pt;
    color: #1a1a2e;
    selection-background-color: #667eea;
}

QTimeEdit:hover {
    border: 2px solid #764ba2;
    background-color: #f8f9fa;
}

QTimeEdit:focus {
    border: 3px solid #667eea;
    background-color: white;
}

QTimeEdit:disabled {
    background-color: #e9ecef;
    color: #adb5bd;
    border: 2px solid #ced4da;
}

/* 按鈕基本樣式 - 使用現代漸層和陰影 */
QPushButton {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 #667eea, stop:1 #764ba2);
    color: white;
    border: none;
    border-radius: 8px;
    padding: 10px 20px;
    font-size: 12pt;
    font-weight: bold;
    font-family: &quot;Microsoft JhengHei&quot;, &quot;PingFang TC&quot;, sans-serif;
}

QPushButton:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 #5568d3, stop:1 #6a3d8f);
}

QPushButton:pressed {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 #4a5ac0, stop:1 #5c347c);
    padding-top: 12px;
    padding-bottom: 8px;
}

QPushButton:disabled {
    background-color: #ced4da;
    color: #6c757d;
}

/* 暫停按鈕 - 使用警告色系 */
QPushButton#pauseButton {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 #f093fb, stop:1 #f5576c);
}

QPushButton#pauseButton:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 #e082ea, stop:1 #e4465b);
}

QPushButton#pauseButton:pressed {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 #d071d9, stop:1 #d3354a);
}

/* 重置按鈕 - 使用危險色系 */
QPushButton#resetButton {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 #fa709a, stop:1 #fee140);
}

QPushButton#resetButton:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 #e95f89, stop:1 #edd02f);
}

QPushButton#resetButton:pressed {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 #d84e78, stop:1 #dcbf1e);
}

/* 群組框樣式 - 增加陰影和深度 */
QGroupBox {
    background-color: white;
    border: 2px solid #dee2e6;
    border-radius: 12px;
    margin-top: 15px;
    padding: 20px 15px 15px 15px;
    font-weight: bold;
    font-size: 11pt;
    color: #495057;
}

QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top left;
    padding: 5px 15px;
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
                                stop:0 #667eea, stop:1 #764ba2);
    color: white;
    border-radius: 6px;
    left: 10px;
    font-family: &quot;Microsoft JhengHei&quot;, &quot;PingFang TC&quot;, sans-serif;
}

/* 文字編輯器樣式 - 改善邊框和內距 */
QTextEdit {
    background-color: white;
    border: 2px solid #dee2e6;
    border-radius: 8px;
    padding: 10px;
    color: #495057;
    font-size: 11pt;
    font-family: &quot;Microsoft JhengHei&quot;, &quot;PingFang TC&quot;, sans-serif;
    selection-background-color: #667eea;
    selection-color: white;
}

QTextEdit:hover {
    border: 2px solid #adb5bd;
}

QTextEdit:focus {
    border: 2px solid #667eea;
    background-color: #f8f9fa;
}

/* 進度條樣式 - 使用漸層和動畫效果 */
QProgressBar {
    border: 2px solid #dee2e6;
    border-radius: 8px;
    text-align: center;
    background-color: #f8f9fa;
    color: #495057;
    font-weight: bold;
    font-size: 11pt;
    height: 30px;
}

QProgressBar::chunk {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
                                stop:0 #667eea, stop:0.5 #764ba2, stop:1 #f093fb);
    border-radius: 6px;
    margin: 1px;
}

/* 狀態標籤特殊樣式 */
QLabel#statusLabel {
    font-size: 13pt;
    font-weight: bold;
    color: #667eea;
    padding: 8px;
}

/* 選單樣式優化 */
QMenuBar {
    background-color: white;
    color: #495057;
    border-bottom: 1px solid #dee2e6;
    font-family: &quot;Microsoft JhengHei&quot;, &quot;PingFang TC&quot;, sans-serif;
}

QMenuBar::item {
    padding: 5px 10px;
    background-color: transparent;
}

QMenuBar::item:selected {
    background-color: #667eea;
    color: white;
    border-radius: 4px;
}

QMenu {
    background-color: white;
    border: 1px solid #dee2e6;
    border-radius: 6px;
    padding: 5px;
}

QMenu::item {
    padding: 8px 25px;
    border-radius: 4px;
}

QMenu::item:selected {
    background-color: #667eea;
    color: white;
}

/* 狀態列樣式 */
QStatusBar {
    background-color: white;
    color: #6c757d;
    border-top: 1px solid #dee2e6;
    font-family: &quot;Microsoft JhengHei&quot;, &quot;PingFang TC&quot;, sans-serif;
}</string>
